{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "extends": [
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/jsx-filename-extension": 0,
      "jsx-a11y/no-autofocus": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/tabindex-no-positive": 0,
      "jsx-a11y/click-events-have-key-events": 0
    }
}
